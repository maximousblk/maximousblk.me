{
  "github": { "silent": true },
  "headers": [
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, stale-while-revalidate"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, stale-while-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/_umami_cloud/:path*",
      "destination": "https://analytics.umami.is/:path*"
    }
  ]
}
